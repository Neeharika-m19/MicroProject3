				<div class="container-fluid">
					<div class="row">
						<div class="col-12">
							<div class="card">
								<div class="card-header">
								  <h3 class="card-title">Kitchen Inventory</h3>
								</div>
								<!-- /.card-header -->
								<div class="card-body">
									<table id="example1" class="table table-bordered table-striped" style="height: auto;">
									  	<thead>
									  		<tr>
												<th>Item ID</th>
												<th>Item Name</th>
												<th>Item Category</th>
												<th>Quantity</th>
												<th>Reorder Level</th>
												<th>Status</th>
									  		</tr>
									  	</thead>
									  	<tbody>
											<% for(var i=0; i<data.length; i++) {%>
												<tr>
													<td><%= data[i].ItemID %></td>
													<td><%= data[i].ItemName %></td>
													<td><%= data[i].ItemCategory %></td>
													<td><%= data[i].Quantity %></td>
													<td><%= data[i].ReorderLevel %></td>
													<td><%= data[i].Status %></td>
												</tr>
											<% } %>
									  	</tbody>
									  	<tfoot>
											<tr>
												<th>Item ID</th>
												<th>Item Name</th>
												<th>Item Category</th>
												<th>Quantity</th>
												<th>Reorder Level</th>
												<th>Status</th>
											</tr>
										</tfoot>
									</table>
								</div>
								<!-- /.card-body -->
